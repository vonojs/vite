{
  "name": "@vonojs/vite",
  "version": "1.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsx build && tsc --declaration --emitDeclarationOnly --project tsconfig.json"
  },
  "files": [
    ".dist"
  ],
  "exports": {
    ".": {
      "import": "./.dist/mod.js",
      "types": "./.dist/mod.d.ts"
    },
    "./server": {
      "types": "./.dist/server.d.ts"
    },
    "./client": {
      "types": "./.dist/client.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "elysiatech": "^1.0.11",
    "sirv": "^3.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "wrangler": "^4.6.0"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^0.1.19",
    "@cloudflare/workers-types": "^4.20250327.0"
  },
  "peerDependencies": {
    "vite": "^6.2.3"
  }
}
