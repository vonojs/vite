{
  "name": "@vonojs/vono",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsx build && tsc --declaration --emitDeclarationOnly --project tsconfig.json"
  },
  "files": [
    ".dist"
  ],
  "exports": {
    ".": {
      "import": "./.dist/mod.js",
      "types": "./.dist/mod.d.ts"
    },
    "./cloudflare": {
      "import": "./.dist/runtime/cloudflare/index.js",
      "types": "./.dist/runtime/cloudflare/index.d.ts"
    },
    "./netlify": {
      "import": "./.dist/runtime/netlify/*.js",
      "types": "./.dist/runtime/netlify/*.d.ts"
    },
    "./node": {
      "import": "./.dist/runtime/node/*.js",
      "types": "./.dist/runtime/node/*.d.ts"
    },
    "./types/server": {
      "types": "./.dist/server.d.ts"
    },
    "./types/client": {
      "types": "./.dist/client.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "wrangler": "^4.6.0",
    "elysiatech": "^1.0.11"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^0.1.19",
    "@cloudflare/workers-types": "^4.20250327.0"
  },
  "peerDependencies": {
    "vite": "^6.2.3"
  }
}
